object test {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
 	val x = 1                                 //> x  : Int = 1
  def increase(i: Int) = i + 1                    //> increase: (i: Int)Int
  val y = increase(x)                             //> y  : Int = 2
	increase(increase(increase(y)))           // res0: Int = 5
}